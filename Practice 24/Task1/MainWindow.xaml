        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Task1"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid Margin="0,0,-449,0">
        <Label Content="Введите строку:" Margin="24,26,676,382"/>
        <TextBox x:Name="inputTextBox" Margin="129,26,470,379"/>

        <Label Content="Значение i:" Margin="27,60,689,344"/>
        <TextBox x:Name="iTextBox" Margin="129,61,470,344"/>

        <Label Content="Значение j:" Margin="30,95,676,308"/>
        <TextBox x:Name="jTextBox" Margin="129,96,470,309"/>

        <Button Content="Поменять местами" Click="SwapButton_Click" Margin="30,184,470,163"/>

        <Label Content="Результат:" Margin="27,131,689,270"/>
        <TextBox x:Name="outputTextBox" Margin="129,135,469,270" IsReadOnly="True"/>
    </Grid>
</Window>
